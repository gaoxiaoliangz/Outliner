{
  "name": "outline.js",
  "version": "0.2.1",
  "description": "Generate outline of your document via JavaScript based on the h tags",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaoxiaoliangz/outline.js.git"
  },
  "author": "gaoxiaoliangz",
  "license": "MIT",
  "homepage": "https://github.com/gaoxiaoliangz/outline.js#readme",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-compass": "^1.0.4",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-image": "^1.2.1",
    "load-grunt-tasks": "^3.4.0",
    "webpack": "^1.12.10"
  },
  "scripts": {
    "watch": "npm run build:js -- --watch & npm run build:css -- --watch",
    "build:css": "node-sass-chokidar src/scss -o dist/css --recursive",
    "build:js": "rollup src/main.js --output.format umd --name \"Outline\" --output.file dist/js/outline.js",
    "build": "npm run build:js & npm run build:css"
  },
  "dependencies": {
    "node-sass-chokidar": "^0.0.3"
  }
}
